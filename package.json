{
  "name": "@examples/next-websockets-starter",
  "version": "10.18.0",
  "private": true,
  "engines": {
    "node": "^16.19.0"
  },
  "scripts": {
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:1-next": "next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "tsx watch src/server/wssServer.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start:next": "next start",
    "start:wss": "node dist/server/wssServer.js",
    "start": "run-p start:*",
    "studio": "prisma studio",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "test": "playwright test",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@dhaiwat10/react-link-preview": "^1.15.0",
    "@next/env": "^12.0.7",
    "@prisma/client": "^4.10.1",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "binance": "^2.5.3",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "image-charts": "^6.1.19",
    "net": "^1.0.2",
    "next": "^13.2.1",
    "node-fetch": "^2.6.9",
    "node-localstorage": "^2.2.1",
    "node-notifier": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-notifications-component": "^4.0.1",
    "react-ts-tradingview-widgets": "^1.0.14",
    "superjson": "^1.7.4",
    "tls": "^0.0.1",
    "tsx": "^3.12.3",
    "ws": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.3.0",
    "@playwright/test": "^1.26.1",
    "@tanstack/react-query-devtools": "^4.3.8",
    "@types/express": "^4.17.17",
    "@types/node": "^18.7.20",
    "@types/node-fetch": "^2.6.3",
    "@types/node-localstorage": "^1.3.0",
    "@types/node-notifier": "^8.0.2",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prisma": "^4.10.1",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
